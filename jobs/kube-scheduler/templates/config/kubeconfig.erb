apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kube-scheduler/config/ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: system:kube-scheduler
  name: kube-scheduler
current-context: kube-scheduler
users:
- name: system:kube-scheduler
  user:
    client-certificate: /var/vcap/jobs/kube-scheduler/config/kube-scheduler-user-cert.pem
    client-key: /var/vcap/jobs/kube-scheduler/config/kube-scheduler-user-key.pem
