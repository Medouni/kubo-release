apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kube-proxy/config/ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: system:kube-proxy
  name: kube-proxy
current-context: kube-proxy
users:
- name: system:kube-proxy
  user:
    client-certificate: /var/vcap/jobs/kube-proxy/config/kube-proxy-user-cert.pem
    client-key: /var/vcap/jobs/kube-proxy/config/kube-proxy-user-key.pem
