apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubernetes-roles/config/ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubernetes-system-specs
  name: kubernetes-system-specs
current-context: kubernetes-system-specs
users:
- name: kubernetes-system-specs
  user:
    client-certificate: /var/vcap/jobs/kubernetes-roles/config/admin-user-cert.pem
    client-key: /var/vcap/jobs/kubernetes-roles/config/admin-user-key.pem
