apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubelet/config/apiserver-ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet
  name: kubelet
current-context: kubelet
users:
- name: kubelet
  user:
    client-certificate: /var/vcap/jobs/kubelet/config/kubelet-user-cert.pem
    client-key: /var/vcap/jobs/kubelet/config/kubelet-user-key.pem
