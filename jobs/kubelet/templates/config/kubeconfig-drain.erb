apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/kubelet/config/apiserver-ca.pem"
    server: https://master.cfcr.internal:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet-drain
  name: kubelet-drain
current-context: kubelet-drain
users:
- name: kubelet-drain
  user:
    client-certificate: /var/vcap/jobs/kubelet/config/kubelet-drain-user-cert.pem
    client-key: /var/vcap/jobs/kubelet/config/kubelet-drain-user-key.pem
    token: "<%= p("drain-api-token") %>"
