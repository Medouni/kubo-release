apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/flanneld/config/ca.pem"
    server: https://master.kubo:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: <%= link('kubernetes-api').p('admin-username') %>
  name: <%= link('kubernetes-api').p('admin-username') %>
current-context: <%= link('kubernetes-api').p('admin-username') %>
users:
- name: <%= link('kubernetes-api').p('admin-username') %>
  user:
    token: "<%= link('kubernetes-api').p('admin-password') %>"
