apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: "/var/vcap/jobs/flanneld/config/ca.pem"
    server: https://master.kubo:8443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: flanneld
  name: flanneld
current-context: flanneld
users:
- name: flanneld
  user:
    token: "<%= p("api-token") %>"
